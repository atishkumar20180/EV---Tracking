<h2>All drivers</h2>
<table class="table">
        <thead>
          <tr>
            <th scope="col">S.No.</th>
            <th scope="col">id</th>
            <th scope="col">name</th>
            <th scope="col">contact</th>
          </tr>
        </thead>
        <tbody>
            <% drivers.forEach((driver, idx)=>{ %>
          <tr>
            <th scope="row"><%= idx+1 %></th>
            <td><%= driver.id %></td>
            <td><%= driver.name %></td>
            <td><%= driver.contact %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
